

<h2>Detalji fitnes centra: </h2>

<form>
    <table>   
        
        @for(int i=0; i<13; i++)
        {
            string oneLine = ViewBag.Data[0];
            string[] konacno = oneLine.Split(';');
            <tr>
                <td>
                    @konacno[i]
                </td>
            </tr>

        }      
        <tr>
            <td>
                <p>
                Povratak na<a href="~/Pocetni/Index"> pocetnu stranicu</a></p>
            </td>
        </tr>
    </table>

</form>

